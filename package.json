{
  "name": "pokeapi",
  "version": "0.0.1",
  "description": "NestJS Pokeapi",
  "author": "@devdbreno <dbrenoti@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "dco": "docker-compose up",
    "build": "nest build",
    "prebuild": "rimraf dist",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "prettier:fix": "prettier --write {src,test}/**/*.ts",
    "eslint:fix": "eslint {src,test}/**/*.ts --fix",
    "test": "jest --passWithNoTests --runInBand",
    "test:staged": "yarn test --findRelatedTests",
    "test:unit": "yarn test --config jest.unit.js",
    "test:unit:cov": "yarn test:unit --coverage",
    "test:unit:watch": "yarn test:unit --watch",
    "test:unit:watch:verbose": "yarn test:unit --watch --verbose",
    "test:e2e": "yarn test --config jest.e2e.js",
    "test:e2e:cov": "yarn test:e2e --coverage",
    "test:e2e:watch": "yarn test:e2e --watch"
  },
  "dependencies": {
    "@nestjs/common": "^7.4.4",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.4",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/typeorm": "^7.1.4",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "pg": "^8.4.2",
    "pokedex-promise-v2": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.2",
    "@nestjs/testing": "^7.4.4",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.5",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.7",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": "14.15.0",
    "yarn": "1.22.5"
  }
}
